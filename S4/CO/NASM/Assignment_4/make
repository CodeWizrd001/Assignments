import os
import sys

name = sys.argv[1]
try :
	kind = int(sys.argv[2])
except :
	kind = 1 

if __name__ == '__main__' :
	os.system('nasm -f elf {0}.asm'.format(name))
	if kind == 1 :
		os.system('ld -melf_i386 {0}.o -o {0}.aos'.format(name))
	elif kind == 2 :
		os.system('gcc -m32 {0}.o -o {0}.aos'.format(name))
